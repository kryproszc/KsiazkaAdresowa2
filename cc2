try:
    ratio_df = calculate_ratios_vectorized(data)
    user_data.ratio_df_p.set(ratio_df)
    user_data.binary_df_p.set(create_binary_df(ratio_df))
except Exception:
    await hide_spinner()
    show_info_modal("❌ Wystąpił błąd podczas przetwarzania danych – nie można wyliczyć współczynników.", error=True)
    return
